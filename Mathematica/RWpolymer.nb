   3.891934737136676*^9}, 3.891934779339408*^9, {3.891934896639029*^9, 
   3.891934924898821*^9}, {3.891935424792107*^9, 3.891935429602565*^9}, {
   3.891936750372052*^9, 3.891936750968449*^9}, {3.892110636886052*^9, 
   3.8921106504740763`*^9}, {3.8971213595709963`*^9, 3.8971213613779373`*^9}},
 
 CellLabel->"In[17]:=",ExpressionUUID->"24636725-bed7-4f70-8d56-d153b4ff8c9e"],

