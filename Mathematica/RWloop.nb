Cell[BoxData[
 RowBox[{"Integrate", "[", 
  RowBox[{
   RowBox[{
    FractionBox["1", "L"], 
    FractionBox["1", 
     RowBox[{" ", "Z"}]], 
    RowBox[{"Exp", "[", 
     RowBox[{"-", 
      FractionBox[
       RowBox[{"Uring", "[", 
        RowBox[{"R", ",", "l"}], "]"}], "kT"]}], "]"}], " ", "4", "\[Pi]", 
    " ", 
    SuperscriptBox["R", "2"], " ", 
    SuperscriptBox["R", "2"]}], ",", 
   RowBox[{"{", 
    RowBox[{"R", ",", "0", ",", "Infinity"}], "}"}], ",", 
